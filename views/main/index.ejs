<div class="row">
  <div class="col-sm-8">
    <h1>Groups</h1>
    <div>
      <h2><a href="/groups/new">Create a new group</a><h2>
    </div>

    <% groups.forEach(function(group) { %>
      <div class="well">
        <h2>
          <a href="/groups/<%= group.id %>"><%= group.title %></a>
        </h2>
        <p>
          <strong><%= group.time %></strong>
        </p>
      </div>
    <% }); %>

  </div>
</div>
